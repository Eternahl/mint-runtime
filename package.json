    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@rollup/plugin-replace": "^2.3.0",
    "babel-jest": "^24.9.0",
    "date-fns": "^2.8.1",
    "event-propagation-path": "^1.0.5",
    "indent-string": "^4.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "rollup": "^1.27.14",
    "rollup-plugin-terser": "^5.1.3",
    "route-parser": "^0.0.5"
